@using Azure.Core
@model StructureApp.Models.Folder

<h1>"Folder" - @Model.Name</h1>

<ul style="list-style-type: none;">
    @foreach (var folder in Model.ChildFolders)
    {
        <li>
            <a href="@ViewBag.Url/@folder.Name">
                @folder.Name
            </a>
        </li>
    }
</ul>